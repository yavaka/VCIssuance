{
  "UserConfig": {
    "Namespaces": [
      {
        "Name": "sbemulatorns",
        "Queues": [
          {
            "Name": "vc-issuance-requests-queue",
            "Properties": {
              "RequiresSession": true,
              "MaxDeliveryCount": 3,
              "LockDurationInSeconds": 30,
              "DeadLetteringOnMessageExpiration": true
            }
          },
          {
            "Name": "vc-revalidation-queue",
            "Properties": {
              "MaxDeliveryCount": 3,
              "LockDurationInSeconds": 30,
              "DeadLetteringOnMessageExpiration": true
            }
          }
        ],
        "Topics": [
          {
            "Name": "vc-issuance-audit-topic",
            "Subscriptions": [
              {
                "Name": "reporting-db-sub"
              },
              {
                "Name": "notifier-sub"
              }
            ]
          }
        ]
      }
    ],
    "Logging": {
      "Level": "Information",
      "Type": "Console"
      // Supported types are "Console" and "File"
    }
  }
}